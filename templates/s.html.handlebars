<head>
  <link rel="stylesheet" type="text/css" href="kube.css">
  <link rel="stylesheet" type="text/css" href="s.css">
</head>
<body>
  <div class="header-splash">

    <h1 class="header"><span><a href="/s.html">iLikeVideogames</a></span></h1>

  </div>
  <div class="tools">
  </div>

    <p class="news" style="font-size:large"><b>Welcome to iLikeVideogames!</b></p>
    <!-- Add two new divs for a personalized games feed and featured games feed  --!>
    
  <row>
    <column cols="6">
      <div class="sales">
        <p>Featured Discounts on Steam!</p>
        {{{gameList}}}
      </div>
    </column>
    <column cols="8">
      <div class="personalizedFeed">
        <p>Your Personalized Feed</p>
        <div class="defaultQueue">
          <p>Get Started by entering your Steam vanity name or user ID number to start!</p>
          <br>
          <form class="forms">
            <row>
              <column cols="8">
                <input type="text" placeholder="Vanity Username or Steam User ID Number">
              </column>
              <column cols="2"> 
                <button upper id="getGames">Go!</button>
              </column>
            </row>
          </form>
        </div>
      </div>
    </column>
  </row>

  <script src="/pageHandler.js"></script>
</body>
